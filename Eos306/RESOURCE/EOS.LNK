
system begin eos_asm
    op osname='Eclipse Operating System'
    libpath %WATCOM%\lib386
    libpath %WATCOM%\lib386\dos
    op stub=..\resource\weos.exe
    F ..\RESOURCE\WSTARTUP
    format os2 le
end

